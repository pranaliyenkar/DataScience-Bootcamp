import matplotlib.pyplot as plt
import pandas as pd
import numpy as np
import seaborn as sns

df=pd.read_csv("Earthquake_all_month.csv")
df.head()

#Bar Chart
plt.figure(figsize=(5,5))
df['status'].value_counts().plot(kind="bar",color=["red","yellow"])
plt.xlabel("Type of Status")
plt.ylabel("Count")
plt.title("Distribution of Status")
plt.show()

#Pie chart
plt.figure(figsize=(5,5))
df['locationSource'].value_counts().plot(kind="pie")
plt.title("Distribution of Status")
plt.show()

#Scatter Chart
plt.figure(figsize=(15,5))
plt.scatter(df['time'].head(5),df['mag'].head(5))
plt.title("Distribution of Status")
plt.show()
